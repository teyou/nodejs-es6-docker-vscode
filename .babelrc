{
  "presets": ["env"],
  "sourceMaps": true,
  "retainLines": true,
  "plugins": [
    ["babel-plugin-root-import", {
      "rootPathSuffix": "src",
      "rootPathPrefix": "~"
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["transform-object-rest-spread"]
  ]
}